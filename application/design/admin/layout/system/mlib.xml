		</reference>
		<reference name="content" type="System_MlibView">
			<action method="setTemplate">system/mlib/add_attribute.phtml</action>
		</reference>
	</system_mlib_add-attribute>
	<!--添加属性end-->
	<!--属性组列表ks
	<system_mlib_attribute-group-list>
		<reference name="content" type="System_MlibView">
