		</reference>
	</system_mlib_attribute-list>
	<!--属性列表end-->
	<!--属性信息ks-->
	<system_mlib_attribute>
		<reference name="content" type="System_MlibView">
			<action method="setTemplate">system/mlib/attribute.phtml</action>
		</reference>
	</system_mlib_attribute>
	<!--属性信息end-->
	<!--添加属性ks-->
	<system_mlib_add-attribute>
