		</reference>
	</system_mlib_model-list>
	<!--型号列表end-->
	<!--型号详细ks-->
	<system_mlib_model>
		<reference name="skin">
			<action method="addCss">system/mlib.css</action>
		</reference>
		<reference name="content" type="System_MlibView">
			<action method="setTemplate">system/mlib/model.phtml</action>
