	<!--属性组信息ks
	<system_mlib_attribute-group>
		<reference name="content" type="System_MlibView">
			<action method="setTemplate">system/mlib/attribute_group.phtml</action>
		</reference>
	</system_mlib_attribute-group>
	属性组信息end-->
	<!--添加组属性ks
	<system_mlib_add-attribute-group>
		<reference name="skin">
			<action method="addCss">system/mlib.css</action>
		</reference>
		<reference name="content" type="System_MlibView">
			<action method="setTemplate">system/mlib/add_attribute_group.phtml</action>
		</reference>
	</system_mlib_add-attribute-group>
